{
  "name": "vs-extension-exporter-cli",
  "version": "1.2.0",
  "description": "Export and/or import vs-code extensions",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node index.ts",
    "list": "npm start -- list",
    "import": "npm start -- import output/my-extensions.json",
    "export": "npm start -- export my-extensions.json",
    "test": "ts-node test/testRunner.ts"
  },
  "bin": {
    "vscode-ext": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianpennino/vs-extension-exporter-cli.git"
  },
  "keywords": [
    "vs-code",
    "code",
    "vscode",
    "export",
    "import",
    "extension",
    "extensions",
    "cli"
  ],
  "author": "sebastianpennino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebastianpennino/vs-extension-exporter-cli/issues"
  },
  "homepage": "https://github.com/sebastianpennino/vs-extension-exporter-cli#readme",
  "engines": {
    "node": ">=24.4.1",
    "npm": ">=11.4.0"
  },
  "dependencies": {
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "24.0.14"
  }
}
